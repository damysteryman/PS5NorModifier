<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PS5NorModifier.MauiHybrid"
             xmlns:components="clr-namespace:PS5NorModifier.MauiHybrid.Components"
             xmlns:shared="clr-namespace:PS5NorModifier.Shared;assembly=PS5NorModifier.Shared"
             x:Class="PS5NorModifier.MauiHybrid.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <!-- <RootComponent Selector="#app" ComponentType="{x:Type components:Routes}" /> -->
            <!-- <RootComponent Selector="app" ComponentType="{x:Type components:Routes}" /> -->
            <RootComponent Selector="app" ComponentType="{x:Type shared:App}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
